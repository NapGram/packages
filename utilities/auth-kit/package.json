{
    "name": "@napgram/auth-kit",
    "version": "0.1.9",
    "description": "Authentication utilities for NapGram",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch"
    },
    "keywords": [
        "napgram",
        "auth"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "dependencies": {
        "@napgram/infra-kit": "workspace:*",
        "@napgram/qq-client": "workspace:*",
        "fastify": "5.6.2"
    },
    "devDependencies": {
        "@fastify/cookie": "^11.0.2",
        "@types/node": "25.0.3",
        "typescript": "5.9.3"
    },
    "publishConfig": {
        "main": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            },
            "./package.json": "./package.json"
        }
    }
}
